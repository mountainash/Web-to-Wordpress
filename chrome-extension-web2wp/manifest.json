{
	"manifest_version": 2,
	"name": "Web to WordPress",
	"version": "1.0.0",
	"description": "Gets content from a web page to pass to a PHP script to create a Wordpress page.",
	"permissions": [
		"activeTab"
	],
	"browser_action": {
		"default_title": "Web to WordPress",
		"default_icon": "logo-128.png",
		"default_popup": "popup.html"
	},
	"icons": {
		"128": "logo-128.png"
	},
	"content_scripts": [
		{
			"matches": [
				"*://*.domainname.com/*"
			],
			"js": [
				"jquery.2.1.1.min.js",
				"content.js"
			]
		}
	]
}